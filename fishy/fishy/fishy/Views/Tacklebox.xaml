<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="fishy.Views.Tacklebox"
             Icon="tackle.png">

    <ContentPage.Content>
        <StackLayout>
			
			<Button Text="My Tackle Box" Clicked="OpenMyTackleBoxPage"/>
			<Button Text="Add New Rod" Clicked="OpenNewRodPage"/>
			<Button Text="Add New Reel" Clicked="OpenNewReelPage"/>
			<Button Text="Add New Line" Clicked="OpenNewLinePage"/>
			<Button Text="Add New Lure" Clicked="OpenNewLurePage"/>
		</StackLayout>
    </ContentPage.Content>
</ContentPage>